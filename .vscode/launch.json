  "version": "0.2.0",
  "configurations": [
    {
      "name": "gRPC Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "grpc",
        "--log-format",
        "text",
        "--log-level",
        "debug",
        "--enable-telemetry"
      ]
    },
    {
      "name": "Wamp-Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "wamp",
        "--log-format",
        "text",
        "--log-level",
        "debug",
        "--enable-telemetry"
      ]
    },
    {
      "name": "Migrate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": ["migrate"]
    },
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    }
  ]
}
